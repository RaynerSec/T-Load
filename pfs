source "$HOME/.msf_default/psf"
clear
setdefault () {
echo "Enter Your Code as seen in website"
read -p ">|" ic
echo ""
echo "Enter Your UserID"
read -p ">|" iud
echo ""
echo "Enter the Remote port as seen in the website"
read -p ">|" rp
echo "rport=\"$rp\"" >> $HOME/.msf_default/psf
echo "c=\"$ic\"" >> $HOME/.msf_default/psf
echo "ud=$(echo \"$iud\")" >> $HOME/.msf_default/psf
reload=$(readlink -f "$0")
exec "$reload"
}
proceed () {
echo ""
echo "Enter the localport to be forwarded"
read -p ">|" lp
clear
echo ""
ssh -N -R 35.196.239.24:$rport:localhost:$lp pfs@35.196.239.24 -i $HOME/T-Load/pfskey -p 22 -o ExitOnForwardFailure=yes -o StrictHostKeyChecking=no -t " sudo pfs.sh $ud $c $rport "
tload
}
if [ -z "$c" ] || [ -z "$ud" ] || [ -z "$rport" ]
then
setdefault
proceed
fi
echo "Your default Code is $c , UserID is $ud and the Remote port is $rport. Continue (Y) with the same or edit it (E) ?"
read -p ">|" ct
if [ $ct = "Y" ] || [ $ct = "y" ]
then
proceed
else
clear
echo "Edits will take place on script restart. So the script will restart after re entering the values"
setdefault
reload=$(readlink -f "$0")
exec "$reload"
fi

